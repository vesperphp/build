{
  "name": "vesperphp/build",
  "type": "project",
  "description": "A PHP framework for quick prototyping.",
  "keywords": ["framework", "php", "prototype", "vesper"],
  "license": "MIT",
  
   "authors": [
      {
          "name": "Rik Janssen",
          "email": "rik@vesperphp.com"
      }
  ],
  
  "require": {
    "vesperphp/framework": "*",
    "scssphp/scssphp": "^1.11.0"
  },
  
   "autoload": {
    "classmap": [
      "app/",
      "vendor/vesperphp/"
    ]
  },

  "config": {
    "optimize-autoloader": true,
    "prefer-stable": false
  }
  
}
